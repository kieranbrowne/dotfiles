{
  "scss": {
      "extends": "css",
      "snippets": {
        "@i": "@include |",
        "@include": "@include |",
        "fs": "font-size: |;",
        "ovh": "overflow: hidden;|",
        "@span${this}": "@include span(${this});|",
        "@bleed($)": "@include bleed($);|",
        "dib": "display: inline-block;|",
      },
      "abbreviations": {
        "ssp" : "@include span(|);",
      }
  }
}
